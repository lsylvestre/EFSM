let a = 
  let rec fact(n) =
    if true then
      let rec aux(n,acc) =
        if n <= 0 then return acc
        if n > 0 then
          aux(n - 1, n * acc) 
      in
      aux(n,1)
  in 
  fact(dataa)
and b = 
  let rec fib(n) =
    let rec aux(n,a,b) =
      if n <= 0 then return a
      if n > 0 then aux(n-1,b,a+b)
    in
      aux(n,1,1)
  in 
  fib(datab) 
in
return a + b
